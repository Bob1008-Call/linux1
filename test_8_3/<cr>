Âø´ÈÄüËøêË°å
func! Compile()
      exec "w"
          if &filetype == 'c'
                  exec "!gcc -g % -o %< && ./%<"
                      elseif &filetype == 'cpp'
                              exec "!g++ -g % -o %< && ./%<"
                                  elseif &filetype == 'cc'
                                          exec "!g++ -g % -o %< && ./%<"
                                              elseif &filetype == 'python'
                                                      exec "!python %"
                                                          elseif &filetype == 'sh'
                                                                  exec "!sh %"
                                                                      endif
                                                                    endfunc


